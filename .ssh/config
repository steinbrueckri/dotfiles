### Hosts MediaMarktSaturn

Host ffm0*
  User mspiousr
  Hostname %h
  ProxyJump jump-ing

Host 172.19.*
  User steinbrueckri_mediamarktsaturn_c
  Hostname %h
  ProxyJump jump-gcp

Host 172.17.*
  User mspiousr
  Hostname %h
  ProxyJump jump-ing

Host gcp04*
  User steinbrueckri_mediamarktsaturn_c
  Hostname %h
  ProxyJump jump-gcp

Host tqa-portal
  User root
  hostname tqa-portal.media-saturn.com
  ProxyJump jump-ing

Host theo.dev.cfgmgmt.media-saturn.com
  ProxyJump jump-ing

Host jump-ing
  # ing04manubn01p.media-saturn.com
  Hostname localhost
  Port 2222
  TCPKeepAlive yes
  ServerAliveInterval 60
  ServerAliveCountMax 10
  ExitOnForwardFailure yes
  ProxyCommand $HOME/bin/iap-tunnel.sh %h %p

Host jump-gcp
  # gcp04manans01p.media-saturn.com
  Hostname localhost
  User steinbrueckri_mediamarktsaturn_c
  Port 2223
  TCPKeepAlive yes
  ServerAliveInterval 60
  ServerAliveCountMax 10
  ExitOnForwardFailure yes
  ProxyCommand $HOME/bin/iap-tunnel.sh %h %p

