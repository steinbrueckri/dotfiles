Host kae-gth1-bar1-ap1
  hostname 192.168.2.231
  User root
  ProxyJump ras3.ts.pixel-combo.de
  PubkeyAcceptedKeyTypes +ssh-rsa
  HostKeyAlgorithms=+ssh-dss

